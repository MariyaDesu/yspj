{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "esnext",
    "module": "esnext",
    "strict": false,
    "jsx": "preserve",
    "moduleResolution": "node",
    "baseUrl": ".",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "node",
      "webpack-env",
      "vite/client",
      "unplugin-icons/types/vue"
    ]
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}